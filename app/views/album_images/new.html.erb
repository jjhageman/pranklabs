<% title "#{@prank.title} | Image Upload" %>
<div id="title">
  <h2><%= @prank.title %><span class="grey"> Image Upload</span></h2>
  <p class="small-link"><%= link_to 'Back to prank', @prank %> | <%= link_to 'Back to album', prank_album_images_path(@prank, @album) %></p>
</div>
<%= error_messages_for :image %>

<%- form_for(@image, :url => prank_album_images_path(:prank_id => @prank), :html => { :multipart => true }) do |f| -%>
  <div class="required">
	<%= f.label :uploaded_data, 'Upload a photo:' %>
	<%= f.file_field :uploaded_data, :class => 'inputText' %>
	<!-- <%= f.file_field :avatar %> -->
  </div>
  <div class="submit">
    <%= f.submit 'Upload' %>
    <%= f.submit "Cancel", :id => "cancel" %>
  </div>
<%- end -%>